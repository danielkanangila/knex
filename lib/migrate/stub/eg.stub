provide: up, down

up = (knex) ->
  <% if (d.tableName) { %>
  knex.schema.createTable "<%= d.tableName %>": t ->
    t.increments()
    <% if (d.columns) { %><%= d.columns %><% } %>
    t.timestamp()
  <% } %>


down = (knex) ->
  <% if (d.tableName) { %>
  knex.schema.dropTable("<%= d.tableName %>")
  <% } %>
